                    <StaticMenuItemStyle ForeColor="#000099" />
                    <Items>
                        <asp:MenuItem Text="Pesquisar RH" Value="Pesquisar RH" 
                            NavigateUrl="~/CadastrarAltaPaciente/RhPesquisa.aspx">
                        </asp:MenuItem>
                        <asp:MenuItem Text="Cadastrar"
