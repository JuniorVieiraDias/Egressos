                                Text="Cadastrar Clinicas" Value="Cadastrar Clinicas"></asp:MenuItem>
                            <asp:MenuItem NavigateUrl="~/Administrativo/AlterarDadosInternacao_UPDATE/RhPesquisa_UPDATE.aspx" 
