                                <asp:MenuItem NavigateUrl="~/Administrativo/Permissao.aspx" 
                                    Text="Permissões do Usuário" Value="Permissões do Usuário"></asp:MenuItem>
                                <asp:MenuItem NavigateUrl="~/Administrativo/AlterarCadastro.aspx" 
